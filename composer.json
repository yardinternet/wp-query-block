{
	"name": "yard/query-block",
	"type": "package",
	"description": "A query block for the Gutenberg editor.",
	"version": "1.0.0",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Yard\\QueryBlock\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:yardinternet/yard-data.git"
		}
	],
	"require": {
		"php": "^8.1",
		"yard/data": "dev-development",
		"jgrossi/corcel": "^7.0",
		"illuminate/auth": "^10.48"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.57"
	},
	"scripts": {
		"format": "./vendor/bin/php-cs-fixer fix"
	},
	"extra": {
		"acorn": {
			"providers": [
				"Yard\\QueryBlock\\QueryBlockServiceProvider"
			]
		}
	}
}
