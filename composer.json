{
	"name": "yard/query-block",
	"type": "package",
	"description": "A query block for the Gutenberg editor.",
	"version": "1.0.0",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Yard\\QueryBlock\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:yardinternet/yard-data.git"
		}
	],
	"require": {
		"php": "^8.0",
		"yard/data": "dev-development",
		"jgrossi/corcel": "^7.0"
	},
	"extra": {
		"acorn": {
			"providers": [
				"Yard\\QueryBlock\\Providers\\QueryBlockServiceProvider"
			],
			"aliases": {
				"Example": "Yard\\QueryBlock\\Facades\\Example"
			}
		}
	}
}
